{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/main-*\"",
    "dev:client": "nodemon --watch dist --exec \"node ../backend/app/static/js/main-*\"",
    "dev:build-server": "webpack --config webpack.server.dev.config.js --watch",
    "dev:build-client": "webpack --config webpack.client.dev.config.js --watch",
    "prod": "npm-run-all --parallel prod:*",
    "prod:server": "--exec \"node build/main-*\"",
    "prod:client": "--exec \"node ../backend/app/static/js/main-*\"",
    "prod:build-server": "webpack --config webpack.server.prod.config.js",
    "prod:build-client": "webpack --config webpack.client.prod.config.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^7.2.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-react-jsx": "^6.1.18",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "body-parser": "^1.14.1",
    "compression": "1.7.0",
    "css-loader": "^0.28.7",
    "express": "4.15.4",
    "file-loader": "^1.1.6",
    "gulp": "^3.9.1",
    "nodemon": "1.12.0",
    "npm-run-all": "4.1.1",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "1.3.3",
    "postcss-smart-import": "^0.7.6",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-render": "^1.1.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.77.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "3.5.6",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0",
    "yargs": "^3.29.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.17",
    "react-hot-loader": "^3.1.3",
    "webpack-bundle-tracker": "^0.2.0",
    "webpack-dev-server": "^2.9.7"
  }
}
