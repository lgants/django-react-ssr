{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/main-*\"",
    "dev:client": "nodemon --watch ../backend/app/static/build --exec \"node scripts/start.js\"",
    "dev:build-server": "webpack --config ./config/webpack.config.server.dev.js --watch",
    "dev:build-client": "webpack --config ./config/webpack.config.client.dev.js --watch",
    "prod": "npm-run-all --parallel prod:*",
    "prod:server": "node build/main-*",
    "prod:client": "node scripts/build.js",
    "prod:build-server": "webpack --config ./config/webpack.config.server.prod.js",
    "prod:build-client": "webpack --config ./config/webpack.config.client.prod.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^7.2.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-react-jsx": "^6.1.18",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.26.0",
    "body-parser": "^1.14.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "compression": "1.7.0",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "express": "4.15.4",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.6",
    "fs-extra": "3.0.1",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "less-loader": "^4.0.5",
    "less": "^2.7.3",
    "nodemon": "1.12.0",
    "npm-run-all": "4.1.1",
    "object-assign": "4.1.1",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "1.3.3",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-smart-import": "^0.7.6",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "16.2.0",
    "react-dev-utils": "^11.0.4",
    "react-dom": "16.2.0",
    "react-render": "^1.1.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-less": "^2.2.12",
    "semantic-ui-react": "^0.77.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.5.6",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0",
    "whatwg-fetch": "2.0.3",
    "yargs": "^3.29.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.17",
    "extract-text-webpack-plugin": "^3.0.2",
    "image-webpack-loader": "^3.4.2",
    "react-hot-loader": "^3.1.3",
    "webpack-bundle-tracker": "^0.2.0",
    "webpack-dev-server": "^2.9.7"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
